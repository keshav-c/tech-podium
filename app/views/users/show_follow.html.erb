<% provide(:title, @title) %>
<div class="row">
  <div class="col-3">
    <%= render partial: 'shared/navigation', locals: { current_user: current_user } %>
  </div>
  <div class="col-9">
    <h2><%= @title %></h2>
    <% if @users.any? %>
      <ul class="users-list">
        <%= render @users %>
      </ul>
    <% end %>
  </div>
</div>
