<% provide(:title, @title) %>
<div class="container">
  <div class="row">
    <div class="col-3">
      <%= render partial: 'shared/navigation' %>
    </div>
    <div class="col-6">
      <h2><%= @title %></h2>
      <% if @users.any? %>
        <ul class="users-list">
          <%= render @users %>
        </ul>
      <% end %>
    </div>
    <div class="col-3">
      user specific info
    </div>
  </div>
</div>
